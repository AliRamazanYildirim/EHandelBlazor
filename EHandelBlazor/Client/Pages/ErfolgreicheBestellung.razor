@page "/bestellung-erfolgreich"
@inject IWarenKorbDienst WarenKorbDienst
<PageTitle>Danke!</PageTitle>
<h3>Danke!</h3>
<span>Danke für ihre Bestellung. Sie können ihre Bestellungen <a href="bestellungen">hier</a> überprüfen.</span>
@code {
    protected override async Task OnInitializedAsync()
    {
        await WarenKorbDienst.GeheZurWarenKorbArtikelAnzahlAsync();
    }
}
